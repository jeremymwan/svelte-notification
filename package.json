{
  "name": "@tadashi/svelte-notification",
  "version": "1.2.3",
  "description": "Svelte component",
  "author": "Thiago Lagden",
  "license": "MIT",
  "keywords": [
    "svelte",
    "notification",
    "component"
  ],
  "bugs": {
    "url": "https://github.com/lagden/svelte-notification/issues"
  },
  "homepage": "https://github.com/lagden/svelte-notification#readme",
  "type": "module",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "npm run lint && npm run test:integration && npm run test:unit",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest --run --coverage",
    "test:ui": "vitest --ui --coverage"
  },
  "peerDependencies": {
    "@tadashi/hex-id": "*",
    "svelte": ">=3.50"
  },
  "devDependencies": {
    "@playwright/test": "1.37.1",
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/kit": "1.25.0",
    "@sveltejs/package": "2.2.2",
    "@vitest/coverage-v8": "0.34.4",
    "@vitest/ui": "0.34.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-svelte": "2.33.1",
    "jsdom": "22.1.0",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "publint": "0.2.2",
    "svelte": "4.2.0",
    "svelte-check": "3.5.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vitest": "0.34.4"
  }
}
